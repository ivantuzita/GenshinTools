@page "/"

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<AuthorizeView>
    <Authorized>
        <button @onclick="Logout">Logout</button>
        <button @onclick="GoToCharacters">Characters</button>
    </Authorized>
    <NotAuthorized>
        <button @onclick="GoToLogin">Login</button>
        <button @onclick="GoToRegister">Register</button>
    </NotAuthorized>
</AuthorizeView>


